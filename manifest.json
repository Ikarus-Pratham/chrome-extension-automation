{
  "manifest_version": 3,
  "name": "Ikarus Delta Automation",
  "version": "1.0",
  "description": "Automates operations on the Ikarus Delta App page",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://app.ikarusdelta.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}